<!DOCTYPE weblogic-rdbms-bean PUBLIC 
 '-//BEA Systems, Inc.//DTD WebLogic 5.1.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls510/dtd/weblogic-rdbms-persistence.dtd'>
<weblogic-rdbms-bean>
  <pool-name>profileDB</pool-name>
  <table-name>PERSON</table-name>
  <attribute-map>
    <object-link>
      <bean-field>mFirstName</bean-field>
      <dbms-column>FIRST_NAME</dbms-column>
    </object-link>
    <object-link>
      <bean-field>mLastName</bean-field>
      <dbms-column>LAST_NAME</dbms-column>
    </object-link>
  </attribute-map>
  <options>
    <use-quoted-names>false</use-quoted-names>
  </options>
</weblogic-rdbms-bean>
